<%= form_for @candidato, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :vocero_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :vocero_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :eleccion_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :eleccion_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :municipio_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :municipio_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :tipo_cargo_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :tipo_cargo_id, :class => 'number_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                candidatos_path, :class => 'btn' %>
  </div>
<% end %>
